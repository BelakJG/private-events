<h1>Create New Event</h1>

<%= form_with model: @event do |form|%>
    <div>
        <%= form.label :event_title %>
        <%= form.text_field :event_title %>
    </div>

    <div>
        <%= form.label :event_date %>
        <%= form.date_field :event_date %>
    </div>

    <%= form.submit %>
<% end %>